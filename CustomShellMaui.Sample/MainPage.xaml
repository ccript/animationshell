<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CustomShellMaui.Sample.MainPage">

    <ScrollView>
        <VerticalStackLayout
            Spacing="25"
            Padding="30,0"
            VerticalOptions="Center">

           

            <Button
                Text="Open Root New 1"
                Clicked="OnGoRootNew1"
                HorizontalOptions="Center" />

            <Button
                Text="Open Push New 1"
                Clicked="OnGoNew1"
                HorizontalOptions="Center" />
            <Frame HeightRequest="2" HasShadow="False" BorderColor="Transparent"/>

                      <Button
                Text="Open Root New 2"
                Clicked="OnGoRootNew2"
                HorizontalOptions="Center" />

            <Button
                Text="Open Push New 2"
                Clicked="OnGoNew2"
                HorizontalOptions="Center" />
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>

